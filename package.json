{
  "name": "stag-dotjs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "config" : { "port" : "8081" },
  "scripts": {
    "dev": "node src/index.js",
    "test": "jest --config ./jest-config.json",
    "clear": "jest --clearCache",
    "bd": "rm -rf dist; webpack --mode=development; cp -r public/* dist",
    "bp": "rm -rf dist; webpack --mode=production; cp -r public/* dist",
    "start": "cd dist; lsof -ti tcp:$npm_package_config_port | xargs kill; python3.8  -m http.server $npm_package_config_port &",
    "lint": "eslint ./"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "js-yaml": "^3.14.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.3.4",
    "mustache": "^4.1.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "ss-loader": "^1.0.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "yaml-loader": "^0.6.0"
  }
}
