            {
                "id": "landing",
                "html": "<div class=\"title\">HI THERE</div>\n<div class=\"sub-title\"></div>\n<form action=\"javascript:void(0);\" class=\"label-fixed-top\">\n    <div>\n        <input class=\"input-text\" id=\"email\" name=\"email\" placeholder=\"Write your email\" type=\"email\" />\n        <label for=\"email\">Write your email</label>\n    </div>\n    <div>\n        <button class=\"btn btn-primary\" disabled=\"\">\n            Valider &nbsp;<i class=\"fa fa-spinner fa-spin off\"></i>\n        </button>\n    </div>\n</form>"
            },
            {
                "id": "register",
                "html": "<div class=\"title\">Création de compte</div>\n<div class=\"sub-title text t-4\"></div>\n\n<form method=\"POST\" accept-charset=\"UTF-8\" novalidate=\"novalidate\" class=\"label-fixed-top\"\n    blocks_order=\"730511bf-4fa5-4a2e-8b0b-b41e83a37d41,89ed0e70-d646-4e0a-b817-1a60be104370\">\n    <input type=\"hidden\" name=\"return_to\" value=\"\" />\n    <input type=\"hidden\" name=\"customer[tags]\" value=\"\" />\n\n    <input type=\"hidden\" name=\"form_type\" value=\"create_customer\" /><input type=\"hidden\" name=\"utf8\" value=\"✓\" />\n\n    <div block-id=\"89ed0e70-d646-4e0a-b817-1a60be104370\">\n        <input required type=\"text\" class=\"input-text\" name=\"customer[email]\" id=\"customer[email]\" placeholder=\"Write your email\" />\n        <label for=customer[email]>Write your email</label>\n    </div>\n    <div block-id=\"730511bf-4fa5-4a2e-8b0b-b41e83a37d41\">\n        <input required type=\"text\" class=\"input-text\" name=\"customer[first_name]\" id=\"customer[first_name]\" placeholder=\"Your first name\" />\n        <label for=customer[first_name]>Your first name</label>\n    </div>\n    <div block-id=\"\">\n        <input required type=\"text\" class=\"input-text\" name=\"customer[password]\" id=\"customer[password]\" placeholder=\"Write your password\" />\n        <label for=customer[password]>Write your password</label>\n        <label class=\"label-error\"><i class=\"fa label-none\"></i>true\n        </label>\n    </div>\n\n    <!-- choiceBlocks -->\n    <!-- lineStyle can be multi-line-->\n\n    <!--date blocks-->\n\n\n    <!-- Subscribe newsletter checkbox The news letter is like choiceList -->\n\n    \n    <div>\n        <button class=\"btn btn-primary\" disabled=\"\">\n            Créer un compte &nbsp;<i class=\"fa fa-spinner fa-spin off\"></i>\n        </button>\n    </div>\n</form>\n<div class=\"action js-back\"><i class=\"fa fa-arrow-left\"></i> Back</div>"
            },
            {
                "id": "signin",
                "html": "<div class=\"title\">Login </div>\n<div class=\"sub-title\"></div>\n\n<form method=\"post\" accept-charset=\"UTF-8\" novalidate=\"novalidate\" class=\"label-fixed-top\"\n    data-login-redirect=\"products\">\n    <input type=\"hidden\" name=\"form_type\" value=\"customer_login\" /><input type=\"hidden\" name=\"utf8\" value=\"✓\" />\n    <div>\n        <input type=\"email\" readonly name=\"customer[email]\" autocomplete=\"email\" class=\"input-text\"\n            placeholder=\"Write your email\" />\n        <label>Write your email</label>\n    </div>\n    <div>\n        <i class=\"fa fa-eye-slash js-show-psw\"></i>\n        <input type=\"password\" name=\"customer[password]\" class=\"input-text\" placeholder=\"Write your password\"\n            autocomplete=\"on\" />\n        <label>Write your password</label>\n        <label class=\"label-error js-signin-err\"><i class=\"fa label-none\"></i>Incorrect password. Please, try again!</label>\n    </div>\n    <button class=\"btn btn-primary\" disabled=\"\">\n        Submit &nbsp;<i class=\"fa fa-spinner fa-spin off\"></i>\n    </button>\n</form>\n<div class=\"action js-recovery\">Recover password</div>\n<div class=\"action js-back\"><i class=\"fa fa-arrow-left\"></i> Back</div>"
            },
            {
                "id": "activate",
                "html": "<div class=\"activate activate-no-tag\">\n    <div class=\"title\">Activate</div>\n    <div class=\"sub-title\">Fill in your email to activate your account.</div>\n    <form method=\"post\" accept-charset=\"UTF-8\" novalidate=\"novalidate\" class=\"label-fixed-top\">\n        <input type=\"hidden\" name=\"form_type\" value=\"customer_login\" /><input type=\"hidden\" name=\"utf8\" value=\"✓\" />\n        <button class=\"btn btn-primary\">\n            Resend the email &nbsp;<i class=\"fa fa-spinner fa-spin off\"></i>\n        </button>\n    </form>\n</div>\n"
            },
            {
                "id": "recovery",
                "html": "<div class=\"title\">Recover password</div>\n<div class=\"sub-title\">Enter your email to receive a reset password link.</div>\n<form method=\"post\" accept-charset=\"UTF-8\" class=\"label-fixed-top\">\n    <input\n        type=\"hidden\"\n        name=\"form_type\"\n        value=\"recover_customer_password\"\n    /><input type=\"hidden\" name=\"utf8\" value=\"✓\" />\n    <div>\n        <input\n            class=\"input-text\"\n            name=\"email\"\n            placeholder=\"Write your email\"\n            type=\"email\"\n        />\n        <label>Write your email</label>\n    </div>\n    <button class=\"btn btn-primary\">\n        Recover &nbsp;<i class=\"fa fa-spinner fa-spin off\"></i>\n    </button>\n</form>\n<div class=\"action js-back\"><i class=\"fa fa-arrow-left\"></i> Back</div>\n"
